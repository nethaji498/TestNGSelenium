package arrayExamples;

public class shortArray {

	public static void main(String[] args) {
		
		int a[]= {2,6,1,4,6,7};
		int temp=0;
		for(int i=0;i<6;i++) {
			
			for(int j=i+1;j<6;j++) {
				if(a[i]>a[j]) {
					a[i]=a[j];
					temp=a[i];
					a[j]=temp;
				}
					
			}
		}
		
		for(int x:a)
			System.out.println(x);

	}

}
